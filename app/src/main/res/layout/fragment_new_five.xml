<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBG"
    android:orientation="vertical">
    <!--tools:context="com.rxjy.des.fragment.NewFiveFragment">-->

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/ll_bottom">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@color/colorWhite"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_marginTop="20px"
                android:padding="20px">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="项目名称"
                    android:textColor="@color/cor595757"
                    android:textSize="@dimen/text_size_m" />

                <EditText
                    android:id="@+id/et_new_xiangmumingcheng"
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="请输入"
                    android:textColor="@color/cor595757"
                    android:textColorHint="@color/line"
                    android:textSize="@dimen/text_size_m" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20px"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/shape_vessel_bg"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="20px">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="办公地点"
                    android:textColor="@color/cor595757"
                    android:textSize="@dimen/text_size_m" />

                <EditText
                    android:id="@+id/et_new_bangongdidian"
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="请输入"
                    android:textColor="@color/cor595757"
                    android:textColorHint="@color/line"
                    android:textSize="@dimen/text_size_m" />

            </LinearLayout>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="78px"
            android:layout_marginTop="20px"
            android:background="@drawable/shape_vessel_bg"
            android:padding="20px">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/tenancy_term"
                android:textColor="@color/cor595757"
                android:textSize="@dimen/text_size_m" />

            <TextView
                android:id="@+id/tv_measure_tenancy_term_end_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:text="       "
                android:textColor="@color/cor595757"
                android:textSize="@dimen/text_size_m" />

            <TextView
                android:id="@+id/tv_measure_tenancy_term_time_line"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/tv_measure_tenancy_term_end_time"
                android:text="--"
                android:textColor="@color/cor595757"
                android:textSize="@dimen/text_size_m" />

            <TextView
                android:id="@+id/tv_measure_tenancy_term_start_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/tv_measure_tenancy_term_time_line"
                android:text="       "
                android:textColor="@color/cor595757"
                android:textSize="@dimen/text_size_m" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_projectattribute"
            android:layout_width="match_parent"
            android:layout_height="78px"
            android:layout_marginTop="20px"
            android:background="@drawable/shape_vessel_bg"
            android:padding="20px">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="项目类型"
                android:textColor="@color/cor595757"
                android:textSize="@dimen/text_size_m" />

            <TextView
                android:id="@+id/tv_projectattribute"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:text="       "
                android:textColor="@color/cor595757"
                android:textSize="@dimen/text_size_m"
                />

        </RelativeLayout>

        <!--<com.rxjy.des.widget.TextGridview-->
            <!--android:id="@+id/tgv_projectattribute"-->
            <!--android:layout_width="fill_parent"-->
            <!--android:layout_height="wrap_content">-->

        <!--</com.rxjy.des.widget.TextGridview>-->

        <com.rxjy.rxcompound.widget.TextGridview
            android:id="@+id/tgv_industrytype"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

        </com.rxjy.rxcompound.widget.TextGridview>

    </LinearLayout>

    </ScrollView>

        <LinearLayout
            android:id="@+id/ll_bottom"
            android:layout_width="fill_parent"
            android:layout_height="80px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@color/corf39800"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="当前您已赚了"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/text_size_m" />

                <TextView
                    android:id="@+id/tv_moneyget"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5px"
                    android:text="￥"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/text_size_l" />

            </LinearLayout>

            <Button
                android:id="@+id/btn_des_dai_measure_sub"
                android:layout_width="match_parent"
                android:layout_height="80px"
                android:layout_weight="2"
                android:background="#2196f3"
                android:text="保存"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_l" />


        </LinearLayout>


    </RelativeLayout>
</LinearLayout>
