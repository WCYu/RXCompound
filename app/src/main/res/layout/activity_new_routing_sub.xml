<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBG"
    android:orientation="vertical">

    <include layout="@layout/include_header_layout" />

    <com.rxjy.rxcompound.widget.CustomListView
        android:id="@+id/lv_new_routing_sub"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="24px"
        android:layout_weight="1"
        android:divider="@color/colorBG"
        android:dividerHeight="24px"/>

    <Button
        android:id="@+id/btn_new_routing_sub"
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:background="@drawable/selector_btn_sub_full"
        android:text="@string/sub"
        android:textColor="@color/colorWhite"
        android:textSize="@dimen/text_size_l" />

</LinearLayout>
