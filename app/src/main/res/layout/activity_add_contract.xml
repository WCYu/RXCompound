                    android:layout_weight="2.3"
                    android:id="@+id/all_Engineering"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="30px"
                    android:hint="请输入"
                    android:textColor="@color/colorThe_dark"

                    android:background="@null"/>
            </LinearLayout>
        </LinearLayout>
