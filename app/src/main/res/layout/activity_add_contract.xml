                android:layout_height="wrap_content"
                android:layout_marginBottom="20px"
                android:layout_marginTop="20px"
                android:orientation="horizontal"
                android:paddingLeft="24px"
                android:weightSum="4">

                <TextView
                    android:layout_weight="1.4"
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:paddingLeft="26px"
                    android:text="水电费(元)"
                    android:textSize="30px"
                    android:textColor="@color/colorBlackLight"
                    />

                <EditText
                    android:layout_weight="2.3"
                    android:id="@+id/waterElectricity"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="30px"
                    android:hint="请输入"

                    android:textColor="@color/colorThe_dark"
                    android:background="@null"/>
            </LinearLayout>
            <View
                android:layout_marginTop="10px"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/timepicker_line"
                android:layout_marginBottom="10px"></View>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20px"
                android:layout_marginTop="20px"
                android:orientation="horizontal"
                android:paddingLeft="24px"
                android:weightSum="4">

                <TextView
                    android:layout_weight="1.4"
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:paddingLeft="26px"
                    android:text="泄水费(元)"
                    android:textSize="30px"
                    android:textColor="@color/colorBlackLight"
            />

                <EditText
                    android:layout_weight="2.3"
                    android:id="@+id/discharge_Fee"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="30px"
                    android:hint="请输入"
                    android:textColor="@color/colorThe_dark"
                    android:background="@null"/>
            </LinearLayout>
            <View
                android:layout_marginTop="10px"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/timepicker_line"
                android:layout_marginBottom="10px"></View>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20px"
                android:layout_marginTop="20px"
                android:orientation="horizontal"
                android:paddingLeft="24px"
                android:weightSum="4">

                <TextView
                    android:layout_weight="1.4"
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:paddingLeft="26px"
                    android:text="空调"
                    android:textSize="30px"
                    android:textColor="@color/colorBlackLight"
                     />

                <TextView
                    android:layout_weight="2.3"
                    android:id="@+id/air_Conditioner"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="30px"
                    android:hint="请选择"

                    android:textColor="@color/colorThe_dark"
                    android:background="@null"/>
            </LinearLayout>
            <View
                android:layout_marginTop="10px"
                android:layout_width="match_parent"
                android:layout_height="1px"
