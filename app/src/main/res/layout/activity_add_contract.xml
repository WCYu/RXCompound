    <Button
        android:id="@+id/submission"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="提交"
        android:background="@color/cor00a0e9"
        android:textColor="@color/white"
        android:textSize="30px"
        />
</LinearLayout>